    </ui:VisualElement>
    <ui:VisualElement name="SALE_DETAILS" style="flex-grow: 1; position: absolute; top: 7%; left: 35%; width: 30%; height: 40%; background-color: rgba(105, 154, 255, 0.62); visibility: visible; display: none;">
