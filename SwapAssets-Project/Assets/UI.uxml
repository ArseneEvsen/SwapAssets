            <ui:Label tabindex="-1" parse-escape-sequences="false" display-tooltip-when-elided="true" name="token_id_label" style="font-size: 18px; -unity-font-style: bold;" />
        </ui:VisualElement>
        <ui:VisualElement name="ASSET_TYPE" style="flex-grow: 1;">
            <ui:Label tabindex="-1" text="Asset type :" parse-escape-sequences="true" display-tooltip-when-elided="true" style="font-size: 18px; -unity-font-style: normal;" />
