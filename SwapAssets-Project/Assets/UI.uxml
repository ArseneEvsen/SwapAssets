        <ui:VisualElement style="flex-grow: 1;">
            <ui:Label tabindex="-1" text="Price :" parse-escape-sequences="true" display-tooltip-when-elided="true" name="price_label" style="font-size: 18px;" />
            <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" name="price_value" enable-rich-text="false" style="font-size: 18px; -unity-font-style: bold;" />
