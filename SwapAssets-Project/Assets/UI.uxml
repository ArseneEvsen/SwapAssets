<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <ui:VisualElement name="NFT_INFOS" style="flex-grow: 1; position: absolute; left: 75%; top: 25%; width: 25%; height: 55%; background-color: rgba(105, 154, 255, 0.62); -unity-text-outline-color: rgba(56, 56, 56, 0.6);">
        <ui:Label tabindex="-1" text="NFT INFOS " parse-escape-sequences="true" display-tooltip-when-elided="true" name="nft_infos_label" style="font-size: 25px; -unity-text-align: upper-center; -unity-font-style: bold;" />
        <ui:VisualElement name="TOKEN_ID" style="flex-grow: 1; align-items: flex-start; justify-content: flex-start; align-self: auto; flex-wrap: nowrap; height: 101px;">
