        <ui:VisualElement style="flex-grow: 1;">
            <ui:TextField picking-mode="Ignore" label="Enter your price (only integer allowed)" value="filler text" name="price_value" />
        </ui:VisualElement>
        <ui:Button text="CANCEL" parse-escape-sequences="true" display-tooltip-when-elided="true" name="cancel_sale" style="margin-bottom: 20px; font-size: 16px; justify-content: flex-end; align-self: center; background-color: rgb(245, 122, 24);" />
        <ui:Button text="CREATE THE SALE" parse-escape-sequences="true" display-tooltip-when-elided="true" name="create_sale" style="margin-bottom: 20px; font-size: 16px; align-self: center; background-color: rgb(89, 197, 88);" />
