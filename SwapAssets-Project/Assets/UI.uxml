        <ui:Label tabindex="-1" text="List to exchange" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Label" style="font-size: 18px; -unity-font-style: bold; -unity-text-align: upper-center;" />
        <ui:VisualElement style="flex-grow: 1;">
            <ui:TextField picking-mode="Ignore" label="Amount of shares to sell :" value="filler text" name="amount_shares" />
