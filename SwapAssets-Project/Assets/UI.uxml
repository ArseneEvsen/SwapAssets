            <ui:Label tabindex="-1" text="Ticket number :" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ticket_number_label" style="-unity-font-style: normal; color: rgb(0, 0, 0); font-size: 18px; height: 30px;" />
