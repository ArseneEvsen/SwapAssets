    <ui:ListView focusable="true" name="SALES_DISPLAY" style="position: absolute; left: 5%; top: 25%; width: 20%; height: 50%; color: rgba(0, 0, 0, 0.62); background-color: rgba(105, 154, 255, 0.62); flex-direction: column-reverse; display: none; visibility: visible;" />
